<?xml version="1.0" encoding="UTF-8"?>
<changeset xmlns="http://resourcesync.org/ns/"
    xmlns:rs="http://resourcesync.org/ns/"
    xmlns:sm="http://www.sitemaps.org/schemas/sitemap/0.9"
    xmlns:atom="http://www.w3.org/2005/Atom"
    xmlns:dc="http://purl.org/dc/terms/">

   <atom:link href="{{ this_changeset_uri }}" rel="self rs:changeset"/>
   <atom:link href="{{ next_changeset_uri }}" rel="next rs:changeset"/>
   
   {% for change in changes %}
   <sm:url>
      <rs:changeid>{{ change.changeid }}</rs:changeid>
      <sm:loc>{{ change.uri }}</sm:loc>
      <sm:lastmod>{{ change.lastmod }}</sm:lastmod>
      <rs:md5>{{ change.md5 }}</rs:md5>
      <rs:changetype>{{ change.changetype }}</rs:changetype>
   </sm:url>
   {% end %}
</changeset>